{
  "name": "athene-ui",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "author": "FruitJ <1848661762@qq.com>",
  "license": "MIT",
  "repository": "https://github.com/AtheneUI/athene-ui.git",
  "scripts": {
    "compile:esm": "cross-env BABEL_ENV=esm gulp compile-tsx-esm && tsc -p ./tsconfig.build.json --outDir es",
    "compile:cjs": "cross-env BABEL_ENV=cjs gulp compile-tsx-cjs && tsc -p ./tsconfig.build.json --outDir lib",
    "compile-less": "gulp compile-less",
    "compile-css": "gulp generator-athene-css && gulp clean-athene-css && gulp distDir-generator-css-sourceMap",
    "build:clean": "node ./scripts/build-clean.script.js",
    "build:umd": "cross-env BABEL_ENV=umd BUILD_ENV=development webpack --config ./config/webpack.prod.conf.js",
    "build:umd-min": "cross-env BABEL_ENV=umd BUILD_ENV=production webpack --config ./config/webpack.prod.conf.js",
    "build:esm": "cross-env BABEL_ENV=esm babel components/index.ts -d es --extensions '.ts' && yarn compile:esm",
    "build:cjs": "cross-env BABEL_ENV=cjs babel components/index.ts -d lib --extensions '.ts' && yarn compile:cjs",
    "build": "yarn build:clean && yarn build:cjs && yarn build:esm && yarn copy-less && yarn copy-css && yarn compile-less && yarn generator-cssInJs && yarn build:umd && yarn build:umd-min && yarn polymerize-less && yarn compile-css",
    "copy-less": "gulp copy-less",
    "copy-css": "gulp copy-css",
    "polymerize-less": "node ./scripts/polymerize-less.script.js",
    "generator-cssInJs-cjs": "cross-env BABEL_ENV=cjs gulp generator-cssInJs-cjs",
    "generator-cssInJs-esm": "cross-env BABEL_ENV=esm gulp generator-cssInJs-esm",
    "generator-cssInJs": "yarn generator-cssInJs-cjs && yarn generator-cssInJs-esm",
    "start": "webpack serve --config ./config/webpack.dev.conf.js",
    "site": "cross-env APP_ROOT=dumi dumi dev",
    "build:site": "cross-env APP_ROOT=dumi dumi build",
    "test": "cross-env BABEL_ENV=esm JEST_ENV=true jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@testing-library/react": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "autoprefixer": "^10.3.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "dumi": "1.1.19",
    "dumi-theme-mobile": "^1.1.7",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^5.0.0",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sourcemaps": "^3.0.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "4.3.8",
    "jest": "^27.0.6",
    "less-loader": "^10.0.1",
    "lint-staged": "^11.1.1",
    "mini-css-extract-plugin": "^2.1.0",
    "postcss-loader": "^6.1.1",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "terser-webpack-plugin": "^5.1.4",
    "ts-node": "^10.1.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.48.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.14.9",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.3.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
